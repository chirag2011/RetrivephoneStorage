<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.retrivephonestorage">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RetrivephoneStorage">
        <activity android:name=".Activity.OtherFileActivity"></activity>
        <activity android:name=".Activity.PptFileActivity" />
        <activity android:name=".Activity.ExcelFileActivity" />
        <activity android:name=".Activity.WordFileActivity" />
        <activity android:name=".Activity.PdfFileActivity" />
        <activity android:name=".Activity.VideoPlayerActivity" />
        <activity android:name=".Activity.AudioPlayerActivity" />
        <activity android:name=".Activity.VideoActivity" />
        <activity android:name=".Activity.MusicActivity" />
        <activity android:name=".Activity.DocumentFileActivity" />
        <activity android:name=".Activity.DocumentShowActivity" />
        <activity android:name=".Activity.ImageActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

</manifest>